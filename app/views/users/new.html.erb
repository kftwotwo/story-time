<div class="login">
  <div class="container">
    <div class="form">

    <h1>Sign up</h1>

      <%= form_for @user  do |f|%>
        <%= f.label :first_name  %>
        <%= f.text_field :first_name %>
        <%= f.label :last_name  %>
        <%= f.text_field :last_name %>
        <%= f.label :email  %>
        <%= f.email_field :email %>
        <%= f.label :password  %>
        <%= f.password_field :password %>

        <%= f.submit %>
      <% end %>



    </div>
  </div>
</div>
